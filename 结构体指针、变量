#include<stdio.h>
struct Yue
{
	char name[20];
	short level;
	char 战斗力[20];
};
int main()
{
	struct Yue x = { "诡秘",999,"不可估计" };
	struct Yue* bt = &x;
	printf(" %s\n %d\n %s\n", (*bt).name, (*bt).level, (*bt).战斗力);
	//.    表示 结构体变量.成员
	printf(" %s\n %d\n %s\n",bt->name,bt->level,bt->战斗力);
    //->   表示 结构体指针->成员



	}
